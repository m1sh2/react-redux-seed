{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true
  },
  "rules": {
    "space-before-function-paren": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "linebreak-style": 0,
    "global-require": 0,
    "eol-last": 0,
    "comma-dangle": 0,
    "spaced-comment": 0,
    "member-access": 0,
    "member-ordering": 2,
    "no-any": 0,
    "no-inferrable-types": 0,
    "no-internal-module": 2,
    "no-var-requires": 0,
    "typedef": 0,
    "typedef-whitespace": 2,
    "ban": 0,
    "forin": 2,
    "label-position": 2,
    "label-undefined": 2,
    "no-arg": 2,
    "no-conditional-assignment": 2,
    "no-console": 1,
    "no-construct": 2,
    "no-debugger": 2,
    "no-duplicate-key": 2,
    "no-duplicate-variable": 2,
    "no-empty": 0,
    "no-eval": 2,
    "no-null-keyword": 0,
    "no-shadowed-variable": 2,
    "no-string-literal": 0,
    "no-switch-case-fall-through": 2,
    "no-unreachable": 2,
    "no-unused-expression": 2,
    "no-unused-variable": 2,
    "no-use-before-declare": 2,
    "no-var-keyword": 2,
    "radix": 2,
    "switch-default": 2,
    "triple-equals": 2,
    "use-strict": 2,
    "eofline": 2,
    "indent": 2,
    "no-require-imports": 0,
    "no-trailing-whitespace": 0,
    "object-literal-sort-keys": 0,
    "trailing-comma": 2,
    "align": 0,
    "class-name": 2,
    "comment-format": 2,
    "interface-name": 0,
    "jsdoc-format": 2,
    "no-consecutive-blank-lines": 0,
    "no-constructor-vars": 0,
    "one-line": 1,
    "quotemark": [2, "single", "backtick"],
    "semicolon": [0, "never"],
    "variable-name": [
      2,
      "check-format",
      "allow-leading-underscore",
      "ban-keywords"
    ],
    "whitespace": [
      2,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "import-destructuring-spacing": 0
  }
}